{
    "executionRoleArn": "arn:aws:iam::505254173539:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "java-website",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "taskRoleArn": "arn:aws:iam::505254173539:role/ECSTaskDemoRole",
    "family": "java-webapp-demo"
}